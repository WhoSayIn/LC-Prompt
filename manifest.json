{
  "name": "LC Prompt",
  "version": "1.10",
  "manifest_version": 3,
  "description": "Adds a context menu to copy a Leetcode question along with custom prompts.",
  "permissions": [
    "contextMenus",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://leetcode.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
